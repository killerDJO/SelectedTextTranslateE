{
    "renderer": {
        "hotkeys": {
            "zoomIn": [
                {
                    "keys": [
                        "Control",
                        "+"
                    ]
                },
                {
                    "keys": [
                        "Control",
                        "="
                    ]
                }
            ],
            "zoomOut": [
                {
                    "keys": [
                        "Control",
                        "-"
                    ]
                }
            ],
            "resetZoom": [
                {
                    "keys": [
                        "Control",
                        "0"
                    ]
                }
            ]
        }
    },
    "engine": {
        "copyDelayMilliseconds": 100,
        "translatePattern": "https://translate.google.com/translate_a/single?client=t&sl={{source-language}}&tl={{target-language}}&hl={{target-language}}&dt=at&dt=bd&dt=ex&dt=ld&dt=md&dt={{forced}}&dt=rw&dt=rm&dt=ss&dt=t&ie=UTF-8&oe=UTF-8&otf=1&ssel=0&tsel=0&kc=4&tk={{hash}}&q={{query}}",
        "playTextPattern": "https://translate.google.com/translate_tts?tl={{language}}&client=t&q={{query}}&tk={{hash}}",
        "baseUrl": "https://translate.google.com",
        "historyRefreshInterval": 30,
        "translationConfigRefreshInterval": 60,
        "userAgent": "Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/68.0.3440.84 Safari/537.36",
        "requestTimeout": 10000,
        "playVolume": 100
    },
    "logging": {
        "maxLogSize": 1048576,
        "logFileName": "log.txt"
    },
    "language": {
        "sourceLanguage": "en",
        "targetLanguage": "ru"
    },
    "search": {
        "searchPattern": "https://www.google.com.ua/search?safe=off&source=hp&q={{query}}"
    },
    "history": {
        "sync": {
            "isContinuousSyncEnabled": true,
            "interval": 60000,
            "powerResumeDelaySeconds": 30,
            "backupOnApplicationStart": true,
            "backupOnApplicationStartNumberToKeep": 1,
            "backupRegularly": false,
            "backupRegularlyIntervalDays": 1,
            "backupRegularlyNumberToKeep": 1
        }
    },
    "scaling": {
        "scaleFactor": 1,
        "scaleTranslationViewOnly": true,
        "scalingStep": 0.05,
        "minScaling": 0.7,
        "maxScaling": 2,
        "verticalResolutionBaseline": 860
    },
    "views": {
        "translation": {
            "width": 300,
            "height": 400,
            "margin": 5,
            "x": null,
            "y": null,
            "renderer": {
                "score": {
                    "highThreshold": 0.05,
                    "mediumThreshold": 0.0025
                },
                "visibility": {
                    "lowScoreThreshold": 0.003,
                    "visibleByDefaultNumber": 7
                },
                "showLoaderDelay": 400,
                "toggleDefinitionHotkey": [
                    {
                        "keys": [
                            "Control",
                            "Shift",
                            "D"
                        ]
                    }
                ],
                "archiveResultHotkey": [
                    {
                        "keys": [
                            "Delete"
                        ]
                    }
                ]
            }
        },
        "history": {
            "width": 70,
            "height": 55,
            "renderer": {
                "pageSize": 20,
                "$columns.key": "column",
                "columns": [
                    {
                        "column": "Translation",
                        "isVisible": true,
                        "weight": 1
                    },
                    {
                        "column": "Input",
                        "isVisible": true,
                        "weight": 1
                    },
                    {
                        "column": "Tags",
                        "isVisible": true,
                        "weight": 0.65
                    },
                    {
                        "column": "TimesTranslated",
                        "isVisible": true,
                        "weight": 0.25
                    },
                    {
                        "column": "LastTranslatedDate",
                        "isVisible": true,
                        "weight": 0.45
                    },
                    {
                        "column": "SourceLanguage",
                        "isVisible": false,
                        "weight": 0.4
                    },
                    {
                        "column": "TargetLanguage",
                        "isVisible": false,
                        "weight": 0.4
                    },
                    {
                        "column": "IsArchived",
                        "isVisible": true,
                        "weight": 0.2
                    }
                ]
            }
        },
        "settings": {
            "width": 35,
            "height": 45
        },
        "about": {
            "width": 500,
            "height": 150
        },
        "engine": {
            "showDelayMilliseconds": 100
        }
    },
    "hotkeys": {
        "playText": [
            {
                "keys": [
                    "Control",
                    "R"
                ]
            }
        ],
        "translate": [
            {
                "keys": [
                    "Control",
                    "T"
                ]
            }
        ],
        "showDefinition": [
            {
                "keys": [
                    "Control",
                    "D"
                ]
            }
        ],
        "inputText": [
            {
                "keys": [
                    "Control",
                    "Shift",
                    "T"
                ]
            }
        ],
        "toggleSuspend": [
            {
                "keys": [
                    "Shift",
                    "Control",
                    "Alt",
                    "S"
                ]
            }
        ]
    },
    "update": {
        "feedUrl": "https://update.electronjs.org/killerDJO/SelectedTextTranslateE",
        "releasesUrl": "https://github.com/killerDJO/SelectedTextTranslateE/releases"
    },
    "firebase": {
        "apiKey": "AIzaSyAuHtBBH6VTFdX12AAOFiIa9xWl7AC2m0A",
        "authDomain": "stte-cad70.firebaseapp.com",
        "projectId": "stte-cad70",
        "firestoreBaseUrl": "https://firestore.googleapis.com/v1beta1"
    },
    "tags": {
        "currentTags": []
    }
}