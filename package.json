{
  "name": "selected-text-translate",
  "private": true,
  "productName": "Selected Text Translate",
  "homepage": "https://github.com/killerDJO/SelectedTextTranslateE",
  "version": "1.3.2",
  "description": "Util to translate selected text.",
  "main": "dist/app/main/main.build.js",
  "author": "killerDJO",
  "license": "ISC",
  "scripts": {
    "renderer-build-dev": "webpack --config ./build-scripts/webpack.renderer.js --mode=development",
    "renderer-build-prod": "webpack --config ./build-scripts/webpack.renderer.js --mode=production",
    "renderer-watch-dev": "webpack --config ./build-scripts/webpack.renderer.js --watch --mode=development",
    "main-build-dev": "webpack --config ./build-scripts/webpack.main.js --mode=development",
    "main-build-prod": "webpack --config ./build-scripts/webpack.main.js --mode=production",
    "main-watch-dev": "webpack --config ./build-scripts/webpack.main.js --watch --mode=development",
    "service-renderer-build-dev": "webpack --config ./build-scripts/webpack.service-renderer.js --mode=development",
    "service-renderer-build-prod": "webpack --config ./build-scripts/webpack.service-renderer.js --mode=production",
    "service-renderer-watch-dev": "webpack --config ./build-scripts/webpack.service-renderer.js --watch --mode=development",
    "full-build-dev": "npm run main-build-dev && npm run renderer-build-dev && npm run service-renderer-build-dev",
    "full-build-prod": "npm run main-build-prod && npm run renderer-build-prod && npm run service-renderer-build-prod",
    "create-release": "npm run native-rebuild && npm run full-build-prod && npm run pack && npm run create-installer",
    "pack": "node build-scripts/package.js",
    "create-installer": "node build-scripts/create-installer.js",
    "native-rebuild": "node build-scripts/build-native.js && npx electron-rebuild --parallel",
    "lint-all": "npx tslint --project src/main && npx tslint --project src/renderer && npx tslint --project src/service-renderer"
  },
