  "license": "ISC",
  "scripts": {
    "renderer-build-dev": "webpack --config ./build/webpack.renderer.js --env.NODE_ENV=dev",
    "renderer-build-prod": "webpack --config ./build/webpack.renderer.js --env.NODE_ENV=prod",
    "renderer-dev-server": "webpack-dev-server --config build/webpack.renderer.js --hot --env.NODE_ENV=dev",
    "main-build-dev": "webpack --config ./build/webpack.main.js --env.NODE_ENV=dev",
    "main-build-prod": "webpack --config ./build/webpack.main.js --env.NODE_ENV=prod",
    "main-watch-dev": "webpack --config ./build/webpack.main.js --watch --env.NODE_ENV=dev",
    "full-build-dev": "npm run main-build-dev && npm run renderer-build-dev",
    "full-build-prod": "npm run main-build-prod && npm run renderer-build-prod",
    "pack-dev": "npm run full-build-dev && pack",
    "pack-prod": "npm run full-build-prod && pack",
    "pack": "node build/package.js",
    "dev": "start npm run main-watch-dev && start npm run renderer-dev-server"
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^1.7.9",
    "vue": "2.5.11",
    "vue-property-decorator": "6.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "electron-packager": "^10.1.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "ts-loader": "^3.2.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "vue-hot-reload-api": "^2.2.4",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.6.0",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "2.5.11",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-node-externals": "^1.6.0"
  }
}