    "main-watch-dev": "webpack --config ./build-scripts/webpack.main.js --watch --mode=development",
    "full-build-dev": "npm run main-build-dev && npm run renderer-build-dev",
    "full-build-prod": "npm run main-build-prod && npm run renderer-build-prod",
    "pack-dev": "npm run full-build-dev && npm run pack",
    "pack-prod": "npm run full-build-prod && npm run pack",
    "pack": "node build-scripts/package.js",
    "rebuild-native": "npx electron-rebuild -f"
  },
